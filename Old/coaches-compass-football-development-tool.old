    <script>
        // Smooth scrolling for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Scroll effect for navigation
        window.addEventListener('scroll', function() {
            const nav = document.querySelector('.header-nav');
            if (window.scrollY > 100) {
                nav.style.backgroundColor = 'rgba(255, 255, 255, 0.95)';
                nav.style.backdropFilter = 'blur(10px)';
            } else {
                nav.style.backgroundColor = '#ffffff';
                nav.style.backdropFilter = 'none';
            }
        });

        // Intersection Observer for animations
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        // Observe elements for animations
        document.querySelectorAll('.scenario-card, .tool-card, .story-card').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(30px)';
            el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
            observer.observe(el);
        });

        // Track events for analytics
        function trackEvent(category, action, label) {
            if (typeof gtag !== 'undefined') {
                gtag('event', action, {
                    event_category: category,
                    event_label: label
                });
            }
        }

        // Track CTA clicks
        document.querySelectorAll('.hero-cta, .sticky-cta, .demo-button').forEach(button => {
            button.addEventListener('click', () => {
                trackEvent('Coaches Compass', 'CTA Click', button.textContent);
            });
        });

        // Track scroll depth
        let maxScroll = 0;
        window.addEventListener('scroll', () => {
            const scrollPercent = Math.round((window.scrollY / (document.body.scrollHeight - window.innerHeight)) * 100);
            if (scrollPercent > maxScroll && scrollPercent % 25 === 0) {
                maxScroll = scrollPercent;
                trackEvent('Coaches Compass', 'Scroll Depth', `${scrollPercent}%`);
            }
        });

        // Hide sticky CTA when in pricing section
        const pricingSection = document.querySelector('.pricing');
        const stickyCTA = document.querySelector('.sticky-cta');
        
        const stickyObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    stickyCTA.style.display = 'none';
                } else {
                    stickyCTA.style.display = 'block';
                }
            });
        });
        
        stickyObserver.observe(pricingSection);

        // Compass spinning animation enhancement
        const compassPreview = document.querySelector('.compass-preview');
        if (compassPreview) {
            compassPreview.addEventListener('mouseenter', function() {
                this.style.transform = 'rotate(5deg) scale(1.02)';
                this.style.transition = 'transform 0.3s ease';
            });
            
            compassPreview.addEventListener('mouseleave', function() {
                this.style.transform = 'rotate(0deg) scale(1)';
            });
        }

        // Add success story hover effects
        document.querySelectorAll('.story-card').forEach(card => {
            card.addEventListener('mouseenter', function() {
                this.style.transform = 'translateY(-5px) scale(1.02)';
                this.style.boxShadow = '0 15px 30px rgba(151, 107, 221, 0.2)';
            });
            
            card.addEventListener('mouseleave', function() {
                this.style.transform = 'translateY(0) scale(1)';
                this.style.boxShadow = '0 5px 15px rgba(0,0,0,0.1)';
            });
        });
    </script>

